            <th>Operação</th>
            <th>Data</th>
        </thead>

        <tbody>
            {{#data}}
            <tr>  
                <form name='form1' action='exibirAleracao' method='post'> 
                    <td>
                        {{nome_usuario}}
                    </td>
                    <td>
                        {{nome_usuario_modificado}}
                    </td>
                    <td>
                        {{id_usuario_modificado}}
                    </td>
                    <td>
                        {{operacao}}
                    </td>
                    <td>
                        {{data}}
                    </td>
                    <!--
                    <td>
                        <button class="btn btn-primary"><i class="fa fa-search"></i> Consultar</button>
                    </td>
                    -->
                </form> 
            </tr>
            {{/data}}
        </tbody>
    </table>
